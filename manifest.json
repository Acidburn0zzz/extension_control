{
   "author": [ "luetage, sjudenim" ],
   "background": {
      "persistent": false,
      "scripts": [ "background.js" ]
   },
   "browser_action": {
      "default_icon": {
         "16": "icons/icn16.png",
         "24": "icons/icn24.png",
         "38": "icons/icn38.png"
      },
      "default_popup": "theme_dark.html",
      "default_title": "Extension Control"
   },
   "description": "Elementary extension manager. Compact design, no superfluous features.",
   "homepage_url": "https://github.com/luetage/extension_control",
   "icons": {
      "128": "icons/icn128.png",
      "16": "icons/icn16.png",
      "48": "icons/icn48.png"
   },
   "key": "",
   "manifest_version": 2,
   "name": "Extension Control",
   "offline_enabled": true,
   "options_ui": {
      "open_in_tab": true,
      "page": "options.html"
   },
   "permissions": [ "management", "storage" ],
   "short_name": "Ext_Control",
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "0.66"
}
